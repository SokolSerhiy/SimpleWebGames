<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<%@ taglib prefix="sf" uri="http://www.springframework.org/tags/form"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Battle Ground</title>
</head>
<body>
	<div style="width: 1200px">
	<c:if test="${userWin}"><h2>You are: <img width="500" height="500" src="https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcTrC4cr0-dVl9yLMsjPshg047xecAMY3rtd1WLpUc848ppTIJaehw"/></h2></c:if>
	<c:if test="${cpuWin}"><h2>You are: <img width="500" height="500" src="data:image/jpeg;base64,"/></h2></c:if>
	<div style="float:left; width: 40%">
	<table>
	<c:forEach var="i" begin="0" end="9">
	<tr>
	<c:forEach var="j" begin="0" end="9">
	<td>
	<c:if test="${battleGround[i][j] eq 0}"><img width="40" height="40" src="http://www.zastavki.com/pictures/640x480/2014/Backgrounds_Blue_background_on_your_desktop_082106_29.jpg"/></c:if>
	<c:if test="${battleGround[i][j] eq 1}"><img width="40" height="40" src="http://img2.goodfon.ru/wallpaper/big/e/8a/tekstura-chernyy-fon-uzor-oboi.jpg"/></c:if>
	<c:if test="${battleGround[i][j] eq 4}"><img width="40" height="40" src="http://st2.depositphotos.com/1259301/6752/i/950/depositphotos_67521827-Freeze-motion-of-red-dust-explosion-isolated-on-black-background.jpg"/></c:if>
	<c:if test="${battleGround[i][j] eq 6}"><img width="40" height="40" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQR9-hXr1XXBusxA5gLUWHkY8I-MTDE7r2lsLwKpMJQQvyQG6G9Aw"/></c:if>
	</td>
	</c:forEach>
	</tr>
	</c:forEach>
	</table>
	</div>
	<div style="float:left">
	<a href="${pageContext.request.contextPath}/battle/new">Start new game</a>
	</div>
	<div style="float:left">
	<table>
	<c:forEach var="i" begin="0" end="9">
	<tr>
	<c:forEach var="j" begin="0" end="9">
	<td><c:if test="${battleGroundCpu[i][j] eq 5}"><img width="40" height="40" src="http://www.zastavki.com/pictures/640x480/2014/Backgrounds_Blue_background_on_your_desktop_082106_29.jpg"/></c:if>
	<c:if test="${battleGroundCpu[i][j] eq 3}"><img width="40" height="40" src="http://img2.goodfon.ru/wallpaper/big/e/8a/tekstura-chernyy-fon-uzor-oboi.jpg"/></c:if>
	<c:if test="${battleGroundCpu[i][j] eq 0}"><a href ="${pageContext.request.contextPath}/battle/${i}${j}"><img width="40" height="40" src="http://fotosale.com.ua/images/products/22/products.22030.1.b.jpg"/></a></c:if>
	<c:if test="${battleGroundCpu[i][j] eq 2}"><a href ="${pageContext.request.contextPath}/battle/${i}${j}"><img width="40" height="40" src="http://fotosale.com.ua/images/products/22/products.22030.1.b.jpg"/></a></c:if>
	</td>
	</c:forEach>
	</tr>
	</c:forEach>
	</table>
	</div>
	</div>
</body>
</html>